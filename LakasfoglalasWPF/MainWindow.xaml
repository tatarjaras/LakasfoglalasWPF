<Window x:Class="LakasfoglalasWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LakasfoglalasWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem Header="Bejeletkezés" Click="MnuLogin_Click" Name="MnuLogin"/>
            <MenuItem Header="Felhasználók" IsEnabled="False" Name="MnuUser">
                <MenuItem Header="Felhasználók listázása"/>
                <MenuItem Header="Felhasználók rögzitése"/>
                <MenuItem Header="Felhasználók módosítása"/>
            </MenuItem>


        </Menu>

    </Grid>
</Window>
